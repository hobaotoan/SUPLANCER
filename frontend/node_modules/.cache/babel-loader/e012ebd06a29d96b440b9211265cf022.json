{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport AvatarComponent from \"@/components/GlobalComponent/AvatarComponent.vue\";\nexport default {\n  name: \"DetailProject\",\n  components: {\n    AvatarComponent\n  },\n  computed: { ...mapGetters({\n      showProject: \"getShowProject\",\n      project: \"getProjectDetail\",\n      freelancer: \"getFreelancerInfor\"\n    })\n  },\n  methods: {\n    closeDetail() {\n      const headerDetail = document.querySelector(\".title-detail h2\");\n      headerDetail.innerText = \"Tổng quan\";\n      this.$store.commit(\"CLOSE_PROJECT\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiEA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,eAAP,MAA4B,kDAA5B;AACA,eAAe;EACXC,IAAI,EAAE,eADK;EAEXC,UAAU,EAAE;IAAEF;EAAF,CAFD;EAGXG,QAAQ,EAAE,EACN,GAAGJ,UAAU,CAAC;MACVK,WAAW,EAAE,gBADH;MAEVC,OAAO,EAAE,kBAFC;MAGVC,UAAU,EAAE;IAHF,CAAD;EADP,CAHC;EAUXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,MAAMC,YAAW,GAAIC,QAAQ,CAACC,aAAT,CAAuB,kBAAvB,CAArB;MACAF,YAAY,CAACG,SAAb,GAAyB,WAAzB;MACA,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,eAAnB;IACH;;EALI;AAVE,CAAf","names":["mapGetters","AvatarComponent","name","components","computed","showProject","project","freelancer","methods","closeDetail","headerDetail","document","querySelector","innerText","$store","commit"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Downloads\\internship-management-main\\internship-management-main\\frontend\\src\\components\\Freelancer\\RegisterProject\\DetailProject.vue"],"sourcesContent":["<template>\n    <div class=\"detail-project\" v-if=\"showProject\">\n        <div\n            class=\"detail__infor row position-relative border rounded-2 p-4 mx-2 animate__fadeIn animate__animated\"\n        >\n            <button\n                type=\"button\"\n                class=\"btn-close position-absolute\"\n                aria-label=\"Close\"\n                @click=\"closeDetail\"\n            ></button>\n            <AvatarComponent\n                :urlAvatar=\"project.urlAvatar\"\n                class=\"col-4 pe-0\"\n            />\n            <div class=\"col-7 ms-3 mt-2\">\n                <div class=\"col-12 mt-0 mb-3\">\n                    <strong>Tên dự án: </strong>{{ project.name }}\n                </div>\n                <div class=\"col-12 my-3\">\n                    <strong>Email: </strong> {{ project.email }}\n                </div>\n                <div class=\"col-12 my-3\">\n                    <strong>Số điện thoại: </strong> {{ project.phoneNumber }}\n                </div>\n                <div class=\"col-12 my-3\">\n                    <strong>Địa chỉ: </strong> {{ project.address }}\n                </div>\n            </div>\n            <div class=\"col-12 my-4 p-0\">\n                <strong>Về đơn vị: </strong> {{ project.message }}\n            </div>\n            <div class=\"col-12 d-flex justify-content-around pt-2\">\n                <button\n                    type=\"button\"\n                    class=\"btn btn-light text-success rounded-5\"\n                    v-if=\"project.id === freelancer.projectId\"\n                    disabled\n                >\n                    Sinh viên đang thực tập\n                </button>\n                <button\n                    type=\"button\"\n                    class=\"btn btn-light text-danger rounded-5\"\n                    v-else-if=\"freelancer.projectId !== null\"\n                    disabled\n                >\n                    Sinh viên đang thực tập tại đơn vị khác\n                </button>\n                <button\n                    v-else\n                    type=\"button\"\n                    class=\"btn\"\n                    style=\"background-color: #2C542F; border:none; color: white;\"\n                    data-bs-toggle=\"modal\"\n                    data-bs-target=\"#sendRegister\"\n                >\n                    <i class=\"bi bi-vector-pen\"></i> &nbsp; Đăng ký\n                </button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport AvatarComponent from \"@/components/GlobalComponent/AvatarComponent.vue\";\nexport default {\n    name: \"DetailProject\",\n    components: { AvatarComponent },\n    computed: {\n        ...mapGetters({\n            showProject: \"getShowProject\",\n            project: \"getProjectDetail\",\n            freelancer: \"getFreelancerInfor\",\n        }),\n    },\n    methods: {\n        closeDetail() {\n            const headerDetail = document.querySelector(\".title-detail h2\");\n            headerDetail.innerText = \"Tổng quan\";\n            this.$store.commit(\"CLOSE_PROJECT\");\n        },\n    },\n};\n</script>\n\n<style scoped>\n.detail__infor {\n    min-height: 300px;\n}\n\n.btn-close {\n    top: 10px;\n    right: 10px;\n}\n.btn-success {\n    height: 40px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}