nt","rule"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Downloads\\internship-management-main\\internship-management-main\\frontend\\src\\components\\Admin\\ManageAccount\\CreateAccount.vue"],"sourcesContent":["<template>\n    <div\n        class=\"modal fade\"\n        id=\"createAccount\"\n        aria-labelledby=\"modalLabel\"\n        aria-hidden=\"true\"\n    >\n        <div class=\"modal-dialog modal-dialog-centered\">\n            <div class=\"modal-content\" style=\"width: 600px\">\n                <div class=\"modal-header text-black\">\n                    <h1 class=\"modal-title fs-5 fw-bold\" id=\"modalLabel\">\n                        Thêm tài khoản\n                    </h1>\n                    <button\n                        type=\"button\"\n                        class=\"btn-close btn-close-black\"\n                        data-bs-dismiss=\"modal\"\n                        aria-label=\"Close\"\n                    ></button>\n                </div>\n                <div class=\"modal-body\">\n                    <form class=\"row p-3\">\n                        <div class=\"infor\">\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"inputName\"\n                                    class=\"col-form-label px-0\"\n                                    ><strong>Tên người dùng:</strong></label\n                                >\n                                <input\n                                    type=\"text\"\n                                    class=\"form-control\"\n                                    id=\"inputName\"\n                                    name=\"name\"\n                                    v-model=\"name\"\n                                />\n                            </div>\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"inputUsername\"\n                                    class=\"col-form-label px-0\"\n                                    ><strong>Tên tài khoản:</strong></label\n                                >\n                                <input\n                                    type=\"text\"\n                                    class=\"form-control\"\n                                    id=\"inputUsername\"\n                                    name=\"username\"\n                                    v-model=\"username\"\n                                />\n                            </div>\n                            <div class=\"mb-3 row\">\n                                <label\n                                for=\"inputPassword\"\n                                    class=\"col-form-label px-0\"\n                                    ><strong>Mật khẩu:</strong></label\n                                >\n                                <input\n                                    type=\"password\"\n                                    class=\"form-control\"\n                                    id=\"inputPassword\"\n                                    name=\"password\"\n                                    v-model=\"password\"\n                                />\n                            </div>\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"inputTypeAccount\"\n                                    class=\"col-form-label px-0\"\n                                    style=\"border:none;\"\n                                    ><strong>Loại tài khoản:</strong></label\n                                >\n                                <select\n                                    class=\"form-select select-type-account\"\n                                    aria-label=\"Select Type Account\"\n                                    id=\"inputTypeAccount\"\n                                    name=\"typeAccount\"\n                                    @change=\"selectTypeAccount($event)\"\n                                    v-model=\"typeAccount\"\n                                >\n                                    <option selected value=\"LEADER\">\n                                        Leader\n                                    </option>\n                                    <option value=\"FREELANCER\">Freelancer</option>\n                                    <option value=\"PROJECT\">\n                                        Đơn vị dự án\n                                    </option>\n                                </select>\n                            </div>\n                            <div\n                                class=\"mb-3 row px-0 semester-schoolYear justify-content-between freelancer-info d-none\"\n                            >\n                                <div class=\"freelancer-class col-4 p-0\">\n                                    <label\n                                        for=\"inputClass\"\n                                        class=\"col-form-label px-0\"\n                                        ><strong>Team:</strong></label\n                                    >\n                                    <input\n                                        type=\"text\"\n                                        class=\"form-control\"\n                                        id=\"inputClass\"\n                                        name=\"freelancerClass\"\n                                        v-model=\"freelancerClass\"\n                                    />\n                                </div>\n                                <div class=\"semester col-3 p-0\">\n                                    <label\n                                        for=\"inputSemester\"\n                                        class=\"col-form-label px-0\"\n                                        ><strong>Học kỳ:</strong></label\n                                    >\n                                    <select\n                                        class=\"form-select select-semester\"\n                                        aria-label=\"Select Semester\"\n                                        id=\"inputSemester\"\n                                        name=\"semester\"\n                                        v-model=\"semester\"\n                                    >\n                                        <option selected value=\"1\">1</option>\n                                        <option value=\"2\">2</option>\n                                        <option value=\"3\">Hè</option>\n                                    </select>\n                                </div>\n                                <div class=\"semester col-4 p-0\">\n                                    <label\n                                        for=\"inputSchoolYear\"\n                                        class=\"col-form-label px-0\"\n                                        ><strong>Niên khóa:</strong></label\n                                    >\n                                    <select\n                                        class=\"form-select select-schoolYear\"\n                                        aria-label=\"Select School Year\"\n                                        id=\"inputSchoolYear\"\n                                        name=\"schoolYear\"\n                                        v-model=\"schoolYear\"\n                                    >\n                                        <option selected value=\"2020-2021\">\n                                            2020-2021\n                                        </option>\n                                        <option value=\"2021-2022\">\n                                            2021-2022\n                                        </option>\n                                        <option value=\"2022-2023\">\n                                            2022-2023\n                                        </option>\n                                        <option value=\"2023-2024\">\n                                            2023-2024\n                                        </option>\n                                        <option value=\"2024-2025\">\n                                            2024-2025\n                                        </option>\n                                    </select>\n                                </div>\n                            </div>\n                        </div>\n                    </form>\n                </div>\n                <div class=\"modal-footer\">\n                    <button\n                        type=\"button\"\n                        class=\"btn text-white\"\n                        style=\"background-color: #A6A6A6; border:none;\"\n                        data-bs-dismiss=\"modal\"\n                    >\n                        Đóng\n                    </button>\n                    <button\n                        type=\"button\"\n                        class=\"btn text-white\"\n                        style=\"background-color: #2C542F; border:none;\"\n                        @click=\"handleCreateAccount\"\n                        data-bs-dismiss=\"modal\"\n                        :disabled=\"\n                            !username || !password || !name || !typeAccount\n                        \"\n                    >\n                        Xác nhận\n                    </button>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport config from \"@/config/index\";\n\nexport default {\n    name: \"CreateAccount\",\n    data() {\n        return {\n            avatar: {\n                path: \"https://cdn-icons-png.flaticon.com/512/1053/1053244.png\",\n                height: 160,\n                width: 160,\n            },\n            username: \"\",\n            password: \"\",\n            name: \"\",\n            typeAccount: \"LEADER\",\n            freelancerClass: \"\",\n            semester: \"1\",\n            schoolYear: \"2020-2021\",\n        };\n    },\n    methods: {\n        selectTypeAccount(event) {\n            const type = event.target.value;\n            const freelancerClass = document.querySelector(\".freelancer-info\");\n            if (type == \"FREELANCER\") {\n                freelancerClass.classList.remove(\"d-none\");\n            } else {\n                if (!freelancerClass.classList.contains(\"d-none\"))\n                    freelancerClass.classList.add(\"d-none\");\n            }\n        },\n        async handleCreateAccount() {\n            const token = localStorage.getItem(\"token\");\n\n            const res = await axios.post(\n                `${config.domain}/admin/create-account`,\n                {\n                    username: this.username.trim(),\n                    password: this.password.trim(),\n                    name: this.name.trim(),\n                    typeAccount: this.typeAccount,\n                    freelancerClass: this.freelancerClass.trim(),\n                    semester: this.semester,\n                    schoolYear: this.schoolYear,\n                },\n                {\n                    headers: {\n                        Authorization: \"Bearer \" + token,\n                    },\n                }\n            );\n\n            this.$store.commit(\"SET_TOAST\", {\n                message: res.data.message,\n                isSuccess: res.data.status,\n            });\n\n            if (res.data.status) {\n                this.$store.commit(\"ADD_ACCOUNT_CURRENT\", {\n                    _id: res.data.newAccount._id,\n                    username: this.username,\n                    name: this.name,\n                    rule: this.typeAccount,\n                });\n            }\n        },\n    },\n};\n</script>\n\n<style scoped>\n.modal-header {\n    background-color: #F2F4E6;\n}\n.modal-body {\n    background-color: #F2F4E6;\n}\n.modal-footer {\n    background-color: #F2F4E6;\n}\n.form-control{\n    border:none;\n}\n.fullName {\n    outline: 0;\n    border-width: 0 0 2px;\n    border-color: blue;\n}\n\n.fullName:focus {\n    border-color: green;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}