{"ast":null,"code":"import axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport ItemFreelancer from \"../../components/Company/FreelancerTraining/ItemFreelancer.vue\";\nimport TrainingDetail from \"../../components/Company/FreelancerTraining/TrainingDetail.vue\";\nimport Statistics from \"../../components/Company/FreelancerTraining/Statistics.vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"FreelancersTraining\",\n  components: {\n    TrainingDetail,\n    Statistics,\n    ItemFreelancer\n  },\n  computed: mapGetters({\n    isShowDetail: \"getShowDetail\",\n    freelancerTraining: \"getFreelancerTraining\",\n    account: \"getAccount\"\n  }),\n\n  async created() {\n    const token = localStorage.getItem(\"token\");\n    this.$store.commit(\"CLOSE_DETAIL\");\n    await this.setAccount();\n    const res = await axios.get(`${config.domain}/company/get-freelancers-training/`, {\n      params: {\n        companyId: this.account.id\n      },\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    this.$store.commit(\"SET_STUDENT_TRAINING\", res.data.result);\n  },\n\n  methods: { ...mapActions([\"setAccount\"])\n  }\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,OAAOC,cAAP,MAA2B,gEAA3B;AACA,OAAOC,cAAP,MAA2B,gEAA3B;AACA,OAAOC,UAAP,MAAuB,4DAAvB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,eAAe;EACXC,IAAI,EAAE,qBADK;EAEXC,UAAU,EAAE;IAAEL,cAAF;IAAkBC,UAAlB;IAA8BF;EAA9B,CAFD;EAIXO,QAAQ,EAAEH,UAAU,CAAC;IACjBI,YAAY,EAAE,eADG;IAEjBC,kBAAkB,EAAE,uBAFH;IAGjBC,OAAO,EAAE;EAHQ,CAAD,CAJT;;EAUX,MAAMC,OAAN,GAAgB;IACZ,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;IACA,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,cAAnB;IACA,MAAM,KAAKC,UAAL,EAAN;IAEA,MAAMC,GAAE,GAAI,MAAMpB,KAAK,CAACqB,GAAN,CACb,GAAEpB,MAAM,CAACqB,MAAO,oCADH,EAEd;MACIC,MAAM,EAAE;QACJC,SAAS,EAAE,KAAKZ,OAAL,CAAaa;MADpB,CADZ;MAIIC,OAAO,EAAE;QAAEC,aAAa,EAAE,YAAYb;MAA7B;IAJb,CAFc,CAAlB;IASA,KAAKG,MAAL,CAAY