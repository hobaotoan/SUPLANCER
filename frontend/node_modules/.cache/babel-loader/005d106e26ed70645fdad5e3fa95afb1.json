{"ast":null,"code":"import FreelancerFilter from \"@/components/FreelancerFilter/FreelancerFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"FreelancersList\",\n  components: {\n    FreelancerFilter\n  },\n  computed: mapGetters({\n    leader: \"getLeaderDetail\",\n    currentList: \"getCurrentList\"\n  }),\n  methods: mapActions([\"setLeaderDetail\"]),\n\n  async created() {\n    // get leaderId query\n    const leaderId = this.$route.params.leaderId;\n    const token = localStorage.getItem(\"token\"); // set leader detail\n\n    this.setLeaderDetail(leaderId);\n    const res = await axios.get(`${config.domain}/ministry/get-freelancer-managed`, {\n      params: {\n        leaderId\n      },\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n    this.$store.commit(\"SET_FREELANCER_LIST\", res.data.result);\n    this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n  }\n\n};","map":{"version":3,"mappings":"AAiDA,OAAOA,gBAAP,MAA6B,oDAA7B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,eAAe;EACXC,IAAI,EAAE,iBADK;EAEXC,UAAU,EAAE;IAAEN;EAAF,CAFD;EAGXO,QAAQ,EAAEH,UAAU,CAAC;IACjBI,MAAM,EAAE,iBADS;IAEjBC,WAAW,EAAE;EAFI,CAAD,CAHT;EAOXC,OAAO,EAAEP,UAAU,CAAC,CAAC,iBAAD,CAAD,CAPR;;EAQX,MAAMQ,OAAN,GAAgB;IACZ;IACA,MAAMC,QAAO,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,QAApC;IAEA,MAAMG,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd,CAJY,CAMZ;;IACA,KAAKC,eAAL,CAAqBN,QAArB;IAEA,MAAMO,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CACb,GAAElB,MAAM,CAACmB,MAAO,kCADH,EAEd;MACIP,MAAM,EAAE;QACJF;MADI,CADZ;MAIIU,OAAO,EAAE;QACLC,aAAa,EAAE,YAAYR;MADtB;IAJb,CAFc,CAAlB;IAWA,KAAKS,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB,EAAqCN,GAAG,CAACO,IAAJ,CAASC,OAA9C;IACA,KAAKH,MAAL,CAAYC,MAAZ,CAAmB,qBAAnB,EAA0CN,GAAG,CAACO,IAAJ,CAASE,MAAnD;IACA,KAAKJ,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;EACH;;AA/BU,CAAf","names":["FreelancerFilter","axios","config","mapActions","mapGetters","name","components","computed","leader","currentList","methods","created","leaderId","$route","params","token","localStorage","getItem","setLeaderDetail","res","get","domain","headers","Authorization","$store","commit","data","classes","result"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Downloads\\internship-management-main\\internship-management-main\\frontend\\src\\components\\Ministry\\FreelancersManagement\\FreelancersList.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"freelancer-list d-flex animate__fadeIn animate__animated\">\n            <div class=\"header-list m-0 p-2 pt-3 col-6\" style=\"color: #2C542F\">\n                <div class=\"d-flex align-items-center\">\n                    <h2 class=\"me-3 fw-bold\">\n                        Danh sách freelancer đang quản lý\n                    </h2>\n                    <img\n                        src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/d/de/OOjs_UI_icon_search-ltr.svg/1200px-OOjs_UI_icon_search-ltr.svg.png\"\n                        alt=\"Lọc sinh viên\"\n                        data-bs-toggle=\"modal\"\n                        data-bs-target=\"#FreelancerFilter\"\n                        class=\"filter-icon\"\n                    />\n                </div>\n\n                <div class=\"line mt-3 mb-2\"></div>\n            </div>\n        </div>\n\n        <table class=\"table table-striped text-center\">\n            <thead>\n                <tr>\n                    <th scope=\"col\">#</th>\n                    <th scope=\"col\">Mã số</th>\n                    <th scope=\"col\">Họ và tên</th>\n                    <th scope=\"col\">Lớp</th>\n                    <th scope=\"col\">Điện thoại</th>\n                    <th scope=\"col\">Email</th>\n                </tr>\n            </thead>\n\n            <tbody style=\"overflow-y: scroll\">\n                <tr v-for=\"(freelancer, index) in currentList\" :key=\"index\">\n                    <th scope=\"row\">{{ index + 1 }}</th>\n                    <td>{{ freelancer.username }}</td>\n                    <td>{{ freelancer.name }}</td>\n                    <td>{{ freelancer.team }}</td>\n                    <td>{{ freelancer.phoneNumber }}</td>\n                    <td>{{ freelancer.email }}</td>\n                </tr>\n            </tbody>\n        </table>\n        <FreelancerFilter />\n    </div>\n</template>\n\n<script>\nimport FreelancerFilter from \"@/components/FreelancerFilter/FreelancerFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n    name: \"FreelancersList\",\n    components: { FreelancerFilter },\n    computed: mapGetters({\n        leader: \"getLeaderDetail\",\n        currentList: \"getCurrentList\",\n    }),\n    methods: mapActions([\"setLeaderDetail\"]),\n    async created() {\n        // get leaderId query\n        const leaderId = this.$route.params.leaderId;\n\n        const token = localStorage.getItem(\"token\");\n\n        // set leader detail\n        this.setLeaderDetail(leaderId);\n\n        const res = await axios.get(\n            `${config.domain}/ministry/get-freelancer-managed`,\n            {\n                params: {\n                    leaderId,\n                },\n                headers: {\n                    Authorization: \"Bearer \" + token,\n                },\n            }\n        );\n        this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n        this.$store.commit(\"SET_FREELANCER_LIST\", res.data.result);\n        this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n    },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}