{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport { mapMutations } from \"vuex\";\nexport default {\n  name: \"IntroduceApp\",\n\n  data() {\n    return {\n      rule: \"\"\n    };\n  },\n\n  computed: mapGetters({\n    isLogin: \"getIsLogin\",\n    account: \"getAccount\",\n    getRouters: \"getRouters\"\n  }),\n  ...mapMutations({\n    login: \"LOGIN\"\n  }),\n\n  routers() {\n    if (this.rule == \"PROJECT\") {\n      return this.getRouters.projectRoutes;\n    } else if (this.rule == \"LEADER\") {\n      return this.getRouters.leaderRoutes;\n    } else if (this.rule == \"MINISTRY\") {\n      return this.getRouters.ministryRoutes;\n    } else if (this.rule == \"ADMIN\") {\n      return this.getRouters.adminRoutes;\n    } else if (this.rule == \"FREELANCER\") {\n      return this.getRouters.freelancerRoutes;\n    }\n\n    return \"\";\n  },\n\n  methods: { ...mapActions([\"logout\"]),\n\n    handleLogout() {\n      this.logout();\n      this.$router.push(\"/login\");\n    }\n\n  },\n\n  created() {\n    const rule = localStorage.getItem(\"rule\");\n    this.rule = rule;\n\n    if (rule) {\n      this.login;\n    }\n  }\n\n};","map":{"version":3,"mappings":";AA0FA,SAASA,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,SAASC,YAAT,QAA6B,MAA7B;AAEA,eAAe;EACXC,IAAI,EAAE,cADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE;IADH,CAAP;EAGH,CANU;;EAOXC,QAAQ,EAAEL,UAAU,CAAC;IAAEM,OAAO,EAAE,YAAX;IAAyBC,OAAO,EAAE,YAAlC;IAA+CC,UAAU,EAAE;EAA3D,CAAD,CAPT;EAQX,GAAGP,YAAY,CAAC;IAAEQ,KAAK,EAAE;EAAT,CAAD,CARJ;;EASPC,OAAO,GAAG;IACN,IAAI,KAAKN,IAAL,IAAa,SAAjB,EAA4B;MACxB,OAAO,KAAKI,UAAL,CAAgBG,aAAvB;IACJ,CAFA,MAEO,IAAI,KAAKP,IAAL,IAAa,QAAjB,EAA2B;MAC9B,OAAO,KAAKI,UAAL,CAAgBI,YAAvB;IACJ,CAFO,MAEA,IAAI,KAAKR,IAAL,IAAa,UAAjB,EAA6B;MAChC,OAAO,KAAKI,UAAL,CAAgBK,cAAvB;IACJ,CAFO,MAEA,IAAI,KAAKT,IAAL,IAAa,OAAjB,EAA0B;MAC7B,OAAO,KAAKI,UAAL,CAAgBM,WAAvB;IACJ,CAFO,MAEA,IAAI,KAAKV,IAAL,IAAa,YAAjB,EAA+B;MAClC,OAAO,KAAKI,UAAL,CAAgBO,gBAAvB;IACJ;;