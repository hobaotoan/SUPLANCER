      <h1 class=\"modal-title fs-5 fw-bold\" id=\"modalLabel\">\r\n                        Cập nhật thông tin tài khoản\r\n                    </h1>\r\n                    <button\r\n                        type=\"button\"\r\n                        class=\"btn-close btn-close-black\"\r\n                        data-bs-dismiss=\"modal\"\r\n                        aria-label=\"Close\"\r\n                    ></button>\r\n                </div>\r\n                <div class=\"modal-body\" style=\"background-color: #F2F4E6;\">\r\n                    <form class=\"row py-2 px-3\">\r\n                        <div class=\"infor col-12\">\r\n                            <div class=\"mb-3 row\">\r\n                                <label\r\n                                    for=\"name\"\r\n                                    class=\"col-form-label px-0 py-2\"\r\n                                    ><strong>Tên người dùng:</strong></label\r\n                                >\r\n                                <div class=\"form-control text-center\" id=\"name\">\r\n                                    {{ accountUpdate.username }}\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 row\">\r\n                                <label\r\n                                    for=\"fullname\"\r\n                                    class=\"col-form-label px-0 py-2\"\r\n                                    ><strong>Tên tài khoản:</strong></label\r\n                                >\r\n                                <input\r\n                                    type=\"text\"\r\n                                    class=\"form-control\"\r\n                                    id=\"fullname\"\r\n                                    v-model=\"accountUpdate.name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"mb-3 row\">\r\n                                <label\r\n                                    for=\"password\"\r\n                                    class=\"col-form-label px-0 py-2\"\r\n                                    ><strong>Mật khẩu:</strong></label\r\n                                >\r\n                                <input\r\n                                    type=\"password\"\r\n                                    class=\"form-control\"\r\n                                    id=\"password\"\r\n                                    v-model=\"password\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n                <div class=\"modal-footer\" style=\"background-color: #F2F4E6;\">\r\n                    <button\r\n                        type=\"button\"\r\n                        class=\"btn text-white\"\r\n                        data-bs-dismiss=\"modal\"\r\n                        style=\"background-color: #A6A6A6;\"\r\n                    >\r\n                        Đóng\r\n                    </button>\r\n                    <button\r\n                        type=\"button\"\r\n                        class=\"btn text-white \"\r\n                        data-bs-dismiss=\"modal\"\r\n                        style=\"background-color: #2C542F;\"\r\n                        @click=\"handleUpdateAccount\"\r\n                        :disabled=\"!accountUpdate.name || !password\"\r\n                    >\r\n                        Lưu\r\n                    </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport axios from \"axios\";\r\nimport config from \"@/config/index.js\";\r\nexport default {\r\n    name: \"EditInforAccount\",\r\n    data() {\r\n        return {\r\n            avatar: {\r\n                path: \"https://images.unsplash.com/photo-1664745027113-0145831af78a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=821&q=80\",\r\n                height: 160,\r\n                width: 160,\r\n            },\r\n            password: \"\",\r\n        };\r\n    },\r\n    methods: {\r\n        selectTypeAccount(event) {\r\n            const type = event.target.value;\r\n            const freelancerTeam = document.querySelector(\r\n                \".freelancer-class-update\"\r\n            );\r\n            if (type == 2) {\r\n                freelancerTeam.classList.remove(\"d-none\");\r\n            } else {\r\n                if (!freelancerTeam.classList.contains(\"d-none\"))\r\n                    freelancerTeam.classList.add(\"d-none\");\r\n            }\r\n        },\r\n        async handleUpdateAccount() {\r\n            try {\r\n                const token = localStorage.getItem(\"token\");\r\n                const res = await axios.patch(\r\n                    `${config.domain}/admin/update-account`,\r\n                    {\r\n                        accountId: this.accountUpdate._id,\r\n                        name: this.accountUpdate.name.trim(),\r\n                        password: this.password.trim(),\r\n                    },\r\n                    { headers: { Authorization: \"Bearer \" + token } }\r\n                );\r\n\r\n                this.$store.commit(\"SET_TOAST\", {\r\n                    message: res.data.message,\r\n                    isSuccess: res.data.status,\r\n                });\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        },\r\n    },\r\n    computed: mapGetters({\r\n        accountUpdate: \"getAccountUpdate\",\r\n    }),\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal-header {\r\n    background-color: #F2F4E6;\r\n}\r\n.fullName {\r\n    outline: 0;\r\n    border-width: 0 0 2px;\r\n    border-color: blue;\r\n}\r\n\r\n.fullName:focus {\r\n    border-color: green;\r\n}\r\n.form-control{\r\n    border:none!important;\r\n}\r\nselect:focus {\r\n  box-shadow: none;\r\n  border:none;\r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}