{"ast":null,"code":"import AvatarComponent from \"@/components/GlobalComponent/AvatarComponent.vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport config from \"@/config\";\nexport default {\n  name: \"SendRegister\",\n\n  data() {\n    return {\n      languages: \"\",\n      projects: \"\",\n      message: \"\"\n    };\n  },\n\n  computed: { ...mapGetters({\n      freelancer: \"getAccount\",\n      project: \"getProjectDetail\"\n    })\n  },\n  methods: { ...mapActions([\"setAccount\"]),\n\n    async sendRegister() {\n      const token = localStorage.getItem(\"token\");\n      const response = await axios.post(`${config.domain}/freelancer/register-project`, {\n        freelancerId: this.freelancer.id,\n        projectId: this.project.id,\n        languages: this.languages,\n        projects: this.projects,\n        message: this.message\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.dispatch(\"setToast\", {\n        isSuccess: true,\n        message: response.data.message\n      });\n    }\n\n  },\n\n  created() {\n    this.setAccount();\n  },\n\n  components: {\n    AvatarComponent\n  }\n};","map":{"version":3,"mappings":"AAwIA,OAAOA,eAAP,MAA4B,kDAA5B;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,EADR;MAEHC,QAAQ,EAAE,EAFP;MAGHC,OAAO,EAAE;IAHN,CAAP;EAKH,CARU;;EASXC,QAAQ,EAAE,EACN,GAAGR,UAAU,CAAC;MAAES,UAAU,EAAE,YAAd;MAA4BC,OAAO,EAAE;IAArC,CAAD;EADP,CATC;EAYXC,OAAO,EAAE,EACL,GAAGZ,UAAU,CAAC,CAAC,YAAD,CAAD,CADR;;IAEL,MAAMa,YAAN,GAAqB;MAEjB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,IAAN,CAClB,GAAEf,MAAM,CAACgB,MAAO,8BADE,EAEnB;QACIC,YAAY,EAAE,KAAKV,UAAL,CAAgBW,EADlC;QAEIC,SAAS,EAAE,KAAKX,OAAL,CAAaU,EAF5B;QAGIf,SAAS,EAAE,KAAKA,SAHpB;QAIIC,QAAQ,EAAE,KAAKA,QAJnB;QAKIC,OAAO,EAAE,KAAKA;MALlB