{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nexport default {\n  name: \"EditInforAccount\",\n\n  data() {\n    return {\n      avatar: {\n        path: \"https://images.unsplash.com/photo-1664745027113-0145831af78a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=821&q=80\",\n        height: 160,\n        width: 160\n      },\n      password: \"\"\n    };\n  },\n\n  methods: {\n    selectTypeAccount(event) {\n      const type = event.target.value;\n      const freelancerClass = document.querySelector(\".freelancer-class-update\");\n\n      if (type == 2) {\n        freelancerClass.classList.remove(\"d-none\");\n      } else {\n        if (!freelancerClass.classList.contains(\"d-none\")) freelancerClass.classList.add(\"d-none\");\n      }\n    },\n\n    async handleUpdateAccount() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const res = await axios.patch(`${config.domain}/admin/update-account`, {\n          accountId: this.accountUpdate._id,\n          name: this.accountUpdate.name.trim(),\n          password: this.password.trim()\n        }, {\n          headers: {\n            Authorization: \"Bearer \" + token\n          }\n        });\n        this.$store.commit(\"SET_TOAST\", {\n          message: res.data.message,\n          isSuccess: res.data.status\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n  computed: mapGetters({\n    accountUpdate: \"getAccountUpdate\"\n  })\n};","map":{"version":3,"mappings":"AAuFA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,eAAe;EACXC,IAAI,EAAE,kBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE;QACJC,IAAI,EAAE,+JADF;QAEJC,MAAM,EAAE,GAFJ;QAGJC,KAAK,EAAE;MAHH,CADL;MAMHC,QAAQ,EAAE;IANP,CAAP;EAQH,CAXU;;EAYXC,OAAO,EAAE;IACLC,iBAAiB,CAACC,KAAD,EAAQ;MACrB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAN,CAAaC,KAA1B