der text-white\">\n                    <h1 class=\"modal-title fs-5 fw-bold\" id=\"modalLabel\">\n                        Cập nhật thông tin tài khoản\n                    </h1>\n                    <button\n                        type=\"button\"\n                        class=\"btn-close btn-close-white\"\n                        data-bs-dismiss=\"modal\"\n                        aria-label=\"Close\"\n                    ></button>\n                </div>\n                <div class=\"modal-body\">\n                    <form class=\"row py-2 px-3\">\n                        <div class=\"infor col-12\">\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"name\"\n                                    class=\"col-form-label px-0 py-2\"\n                                    ><strong>Tên đăng nhập:</strong></label\n                                >\n                                <div class=\"form-control text-center\" id=\"name\">\n                                    {{ accountUpdate.username }}\n                                </div>\n                            </div>\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"fullname\"\n                                    class=\"col-form-label px-0 py-2\"\n                                    ><strong>Tên đầy đủ:</strong></label\n                                >\n                                <input\n                                    type=\"text\"\n                                    class=\"form-control\"\n                                    id=\"fullname\"\n                                    v-model=\"accountUpdate.name\"\n                                />\n                            </div>\n                            <div class=\"mb-3 row\">\n                                <label\n                                    for=\"password\"\n                                    class=\"col-form-label px-0 py-2\"\n                                    ><strong>Mật khẩu:</strong></label\n                                >\n                                <input\n                                    type=\"password\"\n                                    class=\"form-control\"\n                                    id=\"password\"\n                                    v-model=\"password\"\n                                />\n                            </div>\n                        </div>\n                    </form>\n                </div>\n                <div class=\"modal-footer\">\n                    <button\n                        type=\"button\"\n                        class=\"btn btn-secondary\"\n                        data-bs-dismiss=\"modal\"\n                    >\n                        Đóng\n                    </button>\n                    <button\n                        type=\"button\"\n                        class=\"btn btn-primary\"\n                        data-bs-dismiss=\"modal\"\n                        @click=\"handleUpdateAccount\"\n                        :disabled=\"!accountUpdate.name || !password\"\n                    >\n                        Lưu thay đổi\n                    </button>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nexport default {\n    name: \"EditInforAccount\",\n    data() {\n        return {\n            avatar: {\n                path: \"https://images.unsplash.com/photo-1664745027113-0145831af78a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=821&q=80\",\n                height: 160,\n                width: 160,\n            },\n            password: \"\",\n        };\n    },\n    methods: {\n        selectTypeAccount(event) {\n            const type = event.target.value;\n            const freelancerClass = document.querySelector(\n                \".freelancer-class-update\"\n            );\n            if (type == 2) {\n                freelancerClass.classList.remove(\"d-none\");\n            } else {\n                if (!freelancerClass.classList.contains(\"d-none\"))\n                    freelancerClass.classList.add(\"d-none\");\n            }\n        },\n        async handleUpdateAccount() {\n            try {\n                const token = localStorage.getItem(\"token\");\n                const res = await axios.patch(\n                    `${config.domain}/admin/update-account`,\n                    {\n                        accountId: this.accountUpdate._id,\n                        name: this.accountUpdate.name.trim(),\n                        password: this.password.trim(),\n                    },\n                    { headers: { Authorization: \"Bearer \" + token } }\n                );\n\n                this.$store.commit(\"SET_TOAST\", {\n                    message: res.data.message,\n                    isSuccess: res.data.status,\n                });\n            } catch (error) {\n                console.log(error);\n            }\n        },\n    },\n    computed: mapGetters({\n        accountUpdate: \"getAccountUpdate\",\n    }),\n};\n</script>\n\n<style scoped>\n.modal-header {\n    background-color: #1f5caa;\n}\n.fullName {\n    outline: 0;\n    border-width: 0 0 2px;\n    border-color: blue;\n}\n\n.fullName:focus {\n    border-color: green;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}