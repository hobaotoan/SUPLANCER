{"ast":null,"code":"import StudentFilter from \"@/components/StudentFilter/StudentFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"StudentsList\",\n  components: {\n    StudentFilter\n  },\n  computed: mapGetters({\n    teacher: \"getTeacherDetail\",\n    currentList: \"getCurrentList\"\n  }),\n  methods: mapActions([\"setTeacherDetail\"]),\n\n  async created() {\n    // get teacherId query\n    const teacherId = this.$route.params.teacherId;\n    const token = localStorage.getItem(\"token\"); // set teacher detail\n\n    this.setTeacherDetail(teacherId);\n    const res = await axios.get(`${config.domain}/ministry/get-freelancer-managed`, {\n      params: {\n        teacherId\n      },\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n    this.$store.commit(\"SET_STUDENT_LIST\", res.data.result);\n    this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n  }\n\n};","map":{"version":3,"mappings":"AAiDA,OAAOA,aAAP,MAA0B,8CAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEN;EAAF,CAFD;EAGXO,QAAQ,EAAEH,UAAU,CAAC;IACjBI,OAAO,EAAE,kBADQ;IAEjBC,WAAW,EAAE;EAFI,CAAD,CAHT;EAOXC,OAAO,EAAEP,UAAU,CAAC,CAAC,kBAAD,CAAD,CAPR;;EAQX,MAAMQ,OAAN,GAAgB;IACZ;IACA,MAAMC,SAAQ,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,SAArC;IAEA,MAAMG,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd,CAJY,CAMZ;;IACA,KAAKC,gBAAL,CAAsBN,SAAtB;IAEA,MAAMO,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CACb,GAAElB,MAAM,CAACmB,MAAO,kCADH,EAEd;MACIP,MAAM,EAAE;QACJF;MADI,CADZ;MAIIU,OAAO,EAAE;QACLC,aAAa,EAAE,YAAYR;MADtB;IAJb,CAFc,CAAlB;IAWA,KAAKS,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB,EAAqCN,GAAG,CAACO,IAAJ,CAASC,OAA9C;IACA,KAAKH,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;IACA,KAAKJ,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;EACH;;AA/BU,CAAf","names":["StudentFilter","axios","config","mapActions","mapGetters","name","components","computed","teacher","currentList","methods","created","teacherId","$route","params","token","localStorage","getItem","setTeacherDetail","res","get","domain","headers","Authorization","$store","commit","data","classes","result"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Downloads\\internship-management-main\\internship-management-main\\frontend\\src\\components\\Ministry\\StudentsManagement\\StudentsList.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"freelancer-list d-flex animate__fadeIn animate__animated\">\n            <div class=\"header-list m-0 p-2 pt-3 col-6\" style=\"color: #555555\">\n                <div class=\"d-flex align-items-center\">\n                    <h2 class=\"me-3 fw-bold\">\n                        Danh sách sinh viên đang quản lý\n                    </h2>\n                    <img\n                        src=\"https://cdn-icons-png.flaticon.com/512/4426/4426381.png\"\n                        alt=\"Lọc sinh viên\"\n                        data-bs-toggle=\"modal\"\n                        data-bs-target=\"#StudentFilter\"\n                        class=\"filter-icon\"\n                    />\n                </div>\n\n                <div class=\"line mt-3 mb-2\"></div>\n            </div>\n        </div>\n\n        <table class=\"table table-striped text-center\">\n            <thead>\n                <tr>\n                    <th scope=\"col\">#</th>\n                    <th scope=\"col\">Mã số</th>\n                    <th scope=\"col\">Họ và tên</th>\n                    <th scope=\"col\">Lớp</th>\n                    <th scope=\"col\">Điện thoại</th>\n                    <th scope=\"col\">Email</th>\n                </tr>\n            </thead>\n\n            <tbody style=\"overflow-y: scroll\">\n                <tr v-for=\"(freelancer, index) in currentList\" :key=\"index\">\n                    <th scope=\"row\">{{ index + 1 }}</th>\n                    <td>{{ freelancer.username }}</td>\n                    <td>{{ freelancer.name }}</td>\n                    <td>{{ freelancer.class }}</td>\n                    <td>{{ freelancer.phoneNumber }}</td>\n                    <td>{{ freelancer.email }}</td>\n                </tr>\n            </tbody>\n        </table>\n        <StudentFilter />\n    </div>\n</template>\n\n<script>\nimport StudentFilter from \"@/components/StudentFilter/StudentFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n    name: \"StudentsList\",\n    components: { StudentFilter },\n    computed: mapGetters({\n        teacher: \"getTeacherDetail\",\n        currentList: \"getCurrentList\",\n    }),\n    methods: mapActions([\"setTeacherDetail\"]),\n    async created() {\n        // get teacherId query\n        const teacherId = this.$route.params.teacherId;\n\n        const token = localStorage.getItem(\"token\");\n\n        // set teacher detail\n        this.setTeacherDetail(teacherId);\n\n        const res = await axios.get(\n            `${config.domain}/ministry/get-freelancer-managed`,\n            {\n                params: {\n                    teacherId,\n                },\n                headers: {\n                    Authorization: \"Bearer \" + token,\n                },\n            }\n        );\n        this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n        this.$store.commit(\"SET_STUDENT_LIST\", res.data.result);\n        this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n    },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}