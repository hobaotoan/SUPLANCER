{"ast":null,"code":"import FreelancerFilter from \"@/components/FreelancerFilter/FreelancerFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"FreelancersList\",\n  components: {\n    FreelancerFilter\n  },\n  computed: mapGetters({\n    adviser: \"getAdviserDetail\",\n    currentList: \"getCurrentList\"\n  }),\n  methods: mapActions([\"setAdviserDetail\"]),\n\n  async created() {\n    // get adviserId query\n    const adviserId = this.$route.params.adviserId;\n    const token = localStorage.getItem(\"token\"); // set adviser detail\n\n    this.setAdviserDetail(adviserId);\n    const res = await axios.get(`${config.domain}/ministry/get-freelancer-managed`, {\n      params: {\n        adviserId\n      },\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n    this.$store.commit(\"SET_FREELANCER_LIST\", res.data.result);\n    this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n  }\n\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,gBAAP,MAA6B,oDAA7B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,eAAe;EACbC,IAAI,EAAE,iBADO;EAEbC,UAAU,EAAE;IAAEN;EAAF,CAFC;EAGbO,QAAQ,EAAEH,UAAU,CAAC;IACnBI,OAAO,EAAE,kBADU;IAEnBC,WAAW,EAAE;EAFM,CAAD,CAHP;EAObC,OAAO,EAAEP,UAAU,CAAC,CAAC,kBAAD,CAAD,CAPN;;EAQb,MAAMQ,OAAN,GAAgB;IACd;IACA,MAAMC,SAAQ,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,SAArC;IAEA,MAAMG,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd,CAJc,CAMd;;IACA,KAAKC,gBAAL,CAAsBN,SAAtB;IAEA,MAAMO,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CACf,GAAElB,MAAM,CAACmB,MAAO,kCADD,EAEhB;MACEP,MAAM,EAAE;QACNF;MADM,CADV;MAIEU,OAAO,EAAE;QACPC,aAAa,EAAE,YAAYR;MADpB;IAJX,CAFgB,CAAlB;IAWA,KAAKS,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB,EAAqCN,GAAG,CAACO,IAAJ,CAASC,OAA9C;IACA,KAAKH,MAAL,CAAYC,MAAZ,CAAmB,qBAAnB,EAA0CN,GAAG,CAACO,IAAJ,CAASE,MAAnD;IACA,KAAKJ,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;EACD;;AA/BY,CAAf","names":["FreelancerFilter","axios","config","mapActions","mapGetters","name","components","computed","adviser","currentList","methods","created","adviserId","$route","params","token","localStorage","getItem","setAdviserDetail","res","get","domain","headers","Authorization","$store","commit","data","classes","result"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Desktop\\SUPLANCER\\frontend\\src\\components\\Ministry\\FreelancersManagement\\FreelancersList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"freelancer-list d-flex animate__fadeIn animate__animated\">\r\n      <div class=\"header-list m-0 p-2 pt-3 col-6\" style=\"color: #2c542f\">\r\n        <div class=\"d-flex align-items-center\">\r\n          <h2 class=\"me-3 fw-bold\">Danh sách Freelancer đang làm việc</h2>\r\n          <!-- <img\r\n                        src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/d/de/OOjs_UI_icon_search-ltr.svg/1200px-OOjs_UI_icon_search-ltr.svg.png\"\r\n                        alt=\"Lọc sinh viên\"\r\n                        data-bs-toggle=\"modal\"\r\n                        data-bs-target=\"#FreelancerFilter\"\r\n                        class=\"filter-icon\"\r\n                    /> -->\r\n        </div>\r\n\r\n        <div class=\"line mt-3 mb-2\"></div>\r\n      </div>\r\n    </div>\r\n\r\n    <table class=\"table table-striped text-center\">\r\n      <thead>\r\n        <tr>\r\n          <th scope=\"col\">#</th>\r\n          <th scope=\"col\">Mã số</th>\r\n          <th scope=\"col\">Họ và tên</th>\r\n          <th scope=\"col\">Lớp</th>\r\n          <th scope=\"col\">Điện thoại</th>\r\n          <th scope=\"col\">Email</th>\r\n        </tr>\r\n      </thead>\r\n\r\n      <tbody style=\"overflow-y: scroll\">\r\n        <tr v-for=\"(freelancer, index) in currentList\" :key=\"index\">\r\n          <th scope=\"row\">{{ index + 1 }}</th>\r\n          <td>{{ freelancer.username }}</td>\r\n          <td>{{ freelancer.name }}</td>\r\n          <td>{{ freelancer.domain }}</td>\r\n          <td>{{ freelancer.phoneNumber }}</td>\r\n          <td>{{ freelancer.email }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <FreelancerFilter />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport FreelancerFilter from \"@/components/FreelancerFilter/FreelancerFilter.vue\";\r\nimport axios from \"axios\";\r\nimport config from \"@/config/index.js\";\r\nimport { mapActions, mapGetters } from \"vuex\";\r\nexport default {\r\n  name: \"FreelancersList\",\r\n  components: { FreelancerFilter },\r\n  computed: mapGetters({\r\n    adviser: \"getAdviserDetail\",\r\n    currentList: \"getCurrentList\",\r\n  }),\r\n  methods: mapActions([\"setAdviserDetail\"]),\r\n  async created() {\r\n    // get adviserId query\r\n    const adviserId = this.$route.params.adviserId;\r\n\r\n    const token = localStorage.getItem(\"token\");\r\n\r\n    // set adviser detail\r\n    this.setAdviserDetail(adviserId);\r\n\r\n    const res = await axios.get(\r\n      `${config.domain}/ministry/get-freelancer-managed`,\r\n      {\r\n        params: {\r\n          adviserId,\r\n        },\r\n        headers: {\r\n          Authorization: \"Bearer \" + token,\r\n        },\r\n      }\r\n    );\r\n    this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\r\n    this.$store.commit(\"SET_FREELANCER_LIST\", res.data.result);\r\n    this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]},"metadata":{},"sourceType":"module"}