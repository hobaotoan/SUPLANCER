btn-close-white\"\n                    data-bs-dismiss=\"offcanvas\"\n                    aria-label=\"Close\"\n                ></button>\n            </div>\n\n            <div class=\"offcanvas-body mt-5\">\n                <div\n                    class=\"infor-freelancer-header d-flex flex-column align-items-center\"\n                >\n                    <Avatar :urlAvatar=\"account.urlAvatar\" />\n                    <h3 class=\"infor-freelancer-name mt-3 fw-bold\">\n                        {{ account.name }}\n                    </h3>\n                </div>\n\n                <ul class=\"infor-freelancer-detail fs-6 mt-5 ps-0 ms-0\">\n                    <li class=\"infor-freelancer-item mb-2\">\n                        <span class=\"item__title me-3\">\n                            <i class=\"bi bi-person-circle\"></i>\n                        </span>\n                        <span class=\"item__value\">{{ account.username }}</span>\n                    </li>\n                    <li class=\"infor-freelancer-item mb-2\">\n                        <span class=\"item__title me-3\">\n                            <i class=\"bi bi-envelope-fill\"></i>\n                        </span>\n                        <span class=\"item__value\">{{ account.email }}</span>\n                    </li>\n                    <li class=\"infor-freelancer-item mb-2\">\n                        <span class=\"item__title me-3\">\n                            <i class=\"bi bi-phone-fill\"></i>\n                        </span>\n                        <span class=\"item__value\">{{\n                            account.phoneNumber\n                        }}</span>\n                    </li>\n                </ul>\n\n                <button\n                    class=\"btn btn-primary rounded-pill mt-5 d-flex m-auto\"\n                    data-bs-dismiss=\"offcanvas\"\n                    @click=\"toUpdate\"\n                >\n                    <i class=\"bi bi-pencil-square\"></i> &nbsp; Cập nhật thông\n                    tin\n                </button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport Avatar from \"../../GlobalComponent/AvatarComponent.vue\";\n\nexport default {\n    name: \"InforFreelancer\",\n    components: { Avatar },\n    computed: mapGetters({ account: \"getAccount\" }),\n    methods: {\n        toUpdate() {\n            this.$router.push(\n                `/${this.account.rule.toLowerCase()}/update-infor/view`\n            );\n        },\n    },\n};\n</script>\n\n<style>\n.btn-infor {\n    border-top-right-radius: 50%;\n    border-bottom-right-radius: 50%;\n    left: -10px;\n}\n.offcanvas-header {\n    background-color: #1f5caa;\n}\n\n.infor-freelancer-detail {\n    list-style-type: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}