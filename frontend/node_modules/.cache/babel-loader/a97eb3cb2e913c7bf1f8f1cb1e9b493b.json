{"ast":null,"code":"import axios from \"axios\";\nimport config from \"@/config/index.js\";\nexport default {\n  name: \"DashBoard\",\n\n  data() {\n    return {\n      total: 0,\n      adviser: 0,\n      freelancer: 0,\n      project: 0\n    };\n  },\n\n  async created() {\n    const token = localStorage.getItem(\"token\");\n    const response = await axios.get(`${config.domain}/admin/get-all-account`, {\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    const accounts = response.data.result;\n\n    for (let i = 0; i < accounts.length; i++) {\n      if (accounts[i].rule == \"PROJECT\") this.project++;else if (accounts[i].rule == \"ADVISER\") this.adviser++;else if (accounts[i].rule == \"FREELANCER\") this.freelancer++;\n    }\n\n    this.total = this.project + this.adviser + this.freelancer;\n  }\n\n};","map":{"version":3,"mappings":"AAoCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CADF;MAELC,OAAO,EAAE,CAFJ;MAGLC,UAAU,EAAE,CAHP;MAILC,OAAO,EAAE;IAJJ,CAAP;EAMD,CATY;;EAUb,MAAMC,OAAN,GAAgB;IACd,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;IACA,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAN,CAAW,GAAEZ,MAAM,CAACa,MAAO,wBAA3B,EAAoD;MACzEC,OAAO,EAAE;QACPC,aAAa,EAAE,YAAYP;MADpB;IADgE,CAApD,CAAvB;IAKA,MAAMQ,QAAO,GAAIL,QAAQ,CAACT,IAAT,CAAce,MAA/B;;IAEA,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAIF,QAAQ,CAACG,MAA7B,EAAqCD,CAAC,EAAtC,EAA0C;MACxC,IAAIF,QAAQ,CAACE,CAAD,CAAR,CAAYE,IAAZ,IAAoB,SAAxB,EAAmC,KAAKd,OAAL,GAAnC,KACK,IAAIU,QAAQ,CAACE,CAAD,CAAR,CAAYE,IAAZ,IAAoB,SAAxB,EAAmC,KAAKhB,OAAL,GAAnC,KACA,IAAIY,QAAQ,CAACE,CAAD,CAAR,CAAYE,IAAZ,IAAoB,YAAxB,EAAsC,KAAKf,UAAL;IAC7C;;IAEA,KAAKF,KAAL,GAAa,KAAKG,OAAL,GAAe,KAAKF,OAApB,GAA8B,KAAKC,UAAhD;EACD;;AA1BY,CAAf","names":["axios","config","name","data","total","adviser","freelancer","project","created","token","localStorage","getItem","response","get","domain","headers","Authorization","accounts","result","i","length","rule"],"sourceRoot":"","sources":["/Users/docaominh/Desktop/NLNKTPM/SUPLANCER/frontend/src/components/Admin/DashBoard/DashBoard.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard row border-bottom pb-4\">\n    <div class=\"dashboard-item col-12 d-flex justify-content-center\">\n      <div class=\"dashboard-detail col-3 d-flex justify-content-around m-2\">\n        <div class=\"dashboard-infor\">\n          <p class=\"item__name mb-1\">Tổng số tài khoản</p>\n          <h3 class=\"item__quantity fw-bold\">{{ total }}</h3>\n        </div>\n        <img src=\"/imageedit_5_6133938238.png\" alt=\"\" class=\"dashboard__img\" />\n      </div>\n      <div class=\"dashboard-detail col-3 d-flex justify-content-around m-2\">\n        <div class=\"dashboard-infor\">\n          <p class=\"item__name mb-1\">Tài khoản Adviser</p>\n          <h3 class=\"item__quantity fw-bold\">{{ adviser }}</h3>\n        </div>\n        <img src=\"/imageedit_2_9588297617.png\" alt=\"\" class=\"dashboard__img\" />\n      </div>\n      <div class=\"dashboard-detail col-3 d-flex justify-content-around m-2\">\n        <div class=\"dashboard-infor\">\n          <p class=\"item__name mb-1\">Tài khoản Freelancer</p>\n          <h3 class=\"item__quantity fw-bold\">{{ freelancer }}</h3>\n        </div>\n        <img src=\"/imageedit_3_5298534601.png\" alt=\"\" class=\"dashboard__img\" />\n      </div>\n      <div class=\"dashboard-detail col-3 d-flex justify-content-around m-2\">\n        <div class=\"dashboard-infor\">\n          <p class=\"item__name mb-1\">Tài khoản đơn vị dự án</p>\n          <h3 class=\"item__quantity fw-bold\">{{ project }}</h3>\n        </div>\n        <img src=\"/imageedit_5_9156385034.png\" alt=\"\" class=\"dashboard__img\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nexport default {\n  name: \"DashBoard\",\n  data() {\n    return {\n      total: 0,\n      adviser: 0,\n      freelancer: 0,\n      project: 0,\n    };\n  },\n  async created() {\n    const token = localStorage.getItem(\"token\");\n    const response = await axios.get(`${config.domain}/admin/get-all-account`, {\n      headers: {\n        Authorization: \"Bearer \" + token,\n      },\n    });\n    const accounts = response.data.result;\n\n    for (let i = 0; i < accounts.length; i++) {\n      if (accounts[i].rule == \"PROJECT\") this.project++;\n      else if (accounts[i].rule == \"ADVISER\") this.adviser++;\n      else if (accounts[i].rule == \"FREELANCER\") this.freelancer++;\n    }\n\n    this.total = this.project + this.adviser + this.freelancer;\n  },\n};\n</script>\n\n<style scoped>\n.dashboard-detail {\n  background-color: white;\n  box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;\n  padding: 20px;\n  border-radius: 10px;\n}\n.dashboard__img {\n  width: 20%;\n  height: 80%;\n  align-self: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}