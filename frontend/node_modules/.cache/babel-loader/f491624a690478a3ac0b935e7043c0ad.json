{"ast":null,"code":"import TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nimport InforProject from \"../../components/GlobalComponent/InforProject.vue\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  components: {\n    TitleStructure,\n    InforProject\n  },\n  computed: { ...mapGetters({\n      account: \"getAccount\",\n      freelancer: \"getFreelancerInfor\"\n    })\n  },\n  methods: {\n    clearDisabled() {\n      const buttons = document.querySelectorAll(\".manage-internship button\");\n      buttons.forEach(button => {\n        button.disabled = false;\n      });\n    },\n\n    viewInforInternship(event) {\n      this.clearDisabled();\n      event.currentTarget.disabled = true;\n    }\n\n  },\n\n  async created() {\n    await this.$store.dispatch(\"setAccount\");\n    const rule = localStorage.getItem(\"rule\").toLowerCase();\n    const payload = {\n      id: this.account.id,\n      rule\n    }; // Dat cac thong tin thuc tap cua sinh vien vao Store de su dung.\n\n    await this.$store.dispatch(\"setFreelancerInfor\", payload);\n    await this.$store.dispatch(\"setProjectInfor\", payload);\n    await this.$store.dispatch(\"setProgressInfor\", payload);\n    await this.$store.dispatch(\"setProjectAssess\", payload);\n    await this.$store.dispatch(\"setAdviserAssess\", payload);\n  },\n\n  mounted() {\n    const viewProgress = document.querySelector(\".view-progress\");\n    const viewAssessProject = document.querySelector(\".view-assess-project\");\n    const viewAssessAdviser = document.querySelector(\".view-assess-adviser\");\n    this.clearDisabled();\n\n    if (viewProgress) {\n      const btnProgress = document.querySelector(\".btn-progress\");\n      btnProgress.disabled = true;\n    }\n\n    if (viewAssessProject) {\n      const btnProject = document.querySelector(\".btn-project\");\n      btnProject.disabled = true;\n    }\n\n    if (viewAssessAdviser) {\n      const btnAdviser = document.querySelector(\".btn-adviser\");\n      btnAdviser.disabled = true;\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAuFA,OAAOA,cAAP,MAA2B,iDAA3B;AACA,OAAOC,YAAP,MAAyB,mDAAzB;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,eAAe;EACbC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CADC;EAGbG,QAAQ,EAAE,EACR,GAAGF,UAAU,CAAC;MAAEG,OAAO,EAAE,YAAX;MAAyBC,UAAU,EAAE;IAArC,CAAD;EADL,CAHG;EAObC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,MAAMC,OAAM,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,2BAA1B,CAAhB;MACAF,OAAO,CAACG,OAAR,CAAiBC,MAAD,IAAY;QAC1BA,MAAM,CAACC,QAAP,GAAkB,KAAlB;MACD,CAFD;IAGD,CANM;;IAOPC,mBAAmB,CAACC,KAAD,EAAQ;MACzB,KAAKR,aAAL;MACAQ,KAAK,CAACC,aAAN,CAAoBH,QAApB,GAA+B,IAA/B;IACD;;EAVM,CAPI;;EAoBb,MAAMI,OAAN,GAAgB;IACd,MAAM,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,YAArB,CAAN;IACA,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,MAArB,EAA6BC,WAA7B,EAAb;IACA,MAAMC,OAAM,GAAI;MAAEC,EAAE,EAAE,KAAKrB,OAAL,CAAaqB,EAAnB;MAAuBL;IAAvB,CAAhB,CAHc,CAKd;;IACA,MAAM,KAAKF,MAAL,CAAYC,QAAZ,CAAqB,oBAArB,EAA2CK,OAA3C,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,iBAArB,EAAwCK,OAAxC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;EACD,CA/BY;;EAgCbE,OAAO,GAAG;IACR,MAAMC,YAAW,GAAIlB,QAAQ,CAACmB,aAAT,CAAuB,gBAAvB,CAArB;IACA,MAAMC,iBAAgB,GAAIpB,QAAQ,CAACmB,aAAT,CAAuB,sBAAvB,CAA1B;IACA,MAAME,iBAAgB,GAAIrB,QAAQ,CAACmB,aAAT,CAAuB,sBAAvB,CAA1B;IACA,KAAKrB,aAAL;;IACA,IAAIoB,YAAJ,EAAkB;MAChB,MAAMI,WAAU,GAAItB,QAAQ,CAACmB,aAAT,CAAuB,eAAvB,CAApB;MACAG,WAAW,CAAClB,QAAZ,GAAuB,IAAvB;IACF;;IACA,IAAIgB,iBAAJ,EAAuB;MACrB,MAAMG,UAAS,GAAIvB,QAAQ,CAACmB,aAAT,CAAuB,cAAvB,CAAnB;MACAI,UAAU,CAACnB,QAAX,GAAsB,IAAtB;IACF;;IACA,IAAIiB,iBAAJ,EAAuB;MACrB,MAAMG,UAAS,GAAIxB,QAAQ,CAACmB,aAAT,CAAuB,cAAvB,CAAnB;MACAK,UAAU,CAACpB,QAAX,GAAsB,IAAtB;IACF;EACD;;AAjDY,CAAf","names":["TitleStructure","InforProject","mapGetters","components","computed","account","freelancer","methods","clearDisabled","buttons","document","querySelectorAll","forEach","button","disabled","viewInforInternship","event","currentTarget","created","$store","dispatch","rule","localStorage","getItem","toLowerCase","payload","id","mounted","viewProgress","querySelector","viewAssessProject","viewAssessAdviser","btnProgress","btnProject","btnAdviser"],"sourceRoot":"","sources":["C:\\Users\\Admin\\Desktop\\SUPLANCER\\frontend\\src\\views\\freelancer\\FreelancerManage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div\r\n      class=\"p-4 animate__animated animate__fadeIn container\"\r\n      v-if=\"freelancer.projectId !== null\"\r\n    >\r\n      <div class=\"col-12 d-flex\">\r\n        <TitleStructure\r\n          :title=\"`Tên dự án làm việc`\"\r\n          class=\"col-6 me-4\"\r\n        ></TitleStructure>\r\n        <TitleStructure\r\n          :title=\"`Quản lý công việc`\"\r\n          class=\"col-6\"\r\n        ></TitleStructure>\r\n      </div>\r\n      <div>\r\n        <div\r\n          class=\"infor row m-0 pt-3 pb-5 border-bottom justify-content-between\"\r\n        >\r\n          <InforProject></InforProject>\r\n          <div\r\n            class=\"manage-internship col-6 d-flex justify-content-center align-items-center mb-4 mx-auto\"\r\n          >\r\n            <div class=\"col-3 me-5\">\r\n              <router-link\r\n                to=\"/freelancer/manage/view-progress\"\r\n                class=\"router-link\"\r\n              >\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn btn-progress\"\r\n                  @click=\"viewInforInternship($event)\"\r\n                >\r\n                  <i class=\"bi bi-calendar-week fs-2 me-2\"></i>\r\n                  <span>Xem tiến độ công việc</span>\r\n                </button>\r\n              </router-link>\r\n            </div>\r\n            <div class=\"col-3 me-5\">\r\n              <router-link\r\n                to=\"/freelancer/manage/view-assess-project\"\r\n                class=\"router-link\"\r\n              >\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn btn-project\"\r\n                  @click=\"viewInforInternship($event)\"\r\n                >\r\n                  <i class=\"bi bi-clipboard-check fs-2 me-2\"></i>\r\n                  <span>Xem đánh giá của đơn vị dự án</span>\r\n                </button>\r\n              </router-link>\r\n            </div>\r\n            <div class=\"col-3\">\r\n              <router-link\r\n                to=\"/freelancer/manage/view-assess-adviser\"\r\n                class=\"router-link\"\r\n              >\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn btn-adviser\"\r\n                  @click=\"viewInforInternship($event)\"\r\n                >\r\n                  <i class=\"bi bi-person-video3 fs-2 me-2\"></i>\r\n                  <span>Xem đánh giá của Adviser</span>\r\n                </button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <router-view></router-view>\r\n      </div>\r\n    </div>\r\n    <div class=\"mt-5 text-center no-info\" v-else>\r\n      <img\r\n        class=\"mt-5\"\r\n        src=\"https://cdn-icons-png.flaticon.com/512/4076/4076389.png\"\r\n        alt=\"\"\r\n        style=\"width: 300px\"\r\n      />\r\n      <h3 class=\"text-warning mt-4\">Freelancer chưa tham gia dự án</h3>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\r\nimport InforProject from \"../../components/GlobalComponent/InforProject.vue\";\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  components: { TitleStructure, InforProject },\r\n\r\n  computed: {\r\n    ...mapGetters({ account: \"getAccount\", freelancer: \"getFreelancerInfor\" }),\r\n  },\r\n\r\n  methods: {\r\n    clearDisabled() {\r\n      const buttons = document.querySelectorAll(\".manage-internship button\");\r\n      buttons.forEach((button) => {\r\n        button.disabled = false;\r\n      });\r\n    },\r\n    viewInforInternship(event) {\r\n      this.clearDisabled();\r\n      event.currentTarget.disabled = true;\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    await this.$store.dispatch(\"setAccount\");\r\n    const rule = localStorage.getItem(\"rule\").toLowerCase();\r\n    const payload = { id: this.account.id, rule };\r\n\r\n    // Dat cac thong tin thuc tap cua sinh vien vao Store de su dung.\r\n    await this.$store.dispatch(\"setFreelancerInfor\", payload);\r\n    await this.$store.dispatch(\"setProjectInfor\", payload);\r\n    await this.$store.dispatch(\"setProgressInfor\", payload);\r\n    await this.$store.dispatch(\"setProjectAssess\", payload);\r\n    await this.$store.dispatch(\"setAdviserAssess\", payload);\r\n  },\r\n  mounted() {\r\n    const viewProgress = document.querySelector(\".view-progress\");\r\n    const viewAssessProject = document.querySelector(\".view-assess-project\");\r\n    const viewAssessAdviser = document.querySelector(\".view-assess-adviser\");\r\n    this.clearDisabled();\r\n    if (viewProgress) {\r\n      const btnProgress = document.querySelector(\".btn-progress\");\r\n      btnProgress.disabled = true;\r\n    }\r\n    if (viewAssessProject) {\r\n      const btnProject = document.querySelector(\".btn-project\");\r\n      btnProject.disabled = true;\r\n    }\r\n    if (viewAssessAdviser) {\r\n      const btnAdviser = document.querySelector(\".btn-adviser\");\r\n      btnAdviser.disabled = true;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.router-link {\r\n  cursor: default;\r\n  text-decoration: none;\r\n}\r\n\r\n.btn {\r\n  height: 90px;\r\n  width: 180px;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\na {\r\n  color: #95a800;\r\n}\r\n.btn:first-child:active {\r\n  background-color: #95a800 !important;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}