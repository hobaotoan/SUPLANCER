{"ast":null,"code":"import TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nimport InforProject from \"../../components/GlobalComponent/InforProject.vue\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  components: {\n    TitleStructure,\n    InforProject\n  },\n  computed: { ...mapGetters({\n      account: \"getAccount\",\n      freelancer: \"getFreelancerInfor\"\n    })\n  },\n  methods: {\n    clearDisabled() {\n      const buttons = document.querySelectorAll(\".manage-internship button\");\n      buttons.forEach(button => {\n        button.disabled = false;\n      });\n    },\n\n    viewInforInternship(event) {\n      this.clearDisabled();\n      event.currentTarget.disabled = true;\n    }\n\n  },\n\n  async created() {\n    await this.$store.dispatch(\"setAccount\");\n    const rule = localStorage.getItem(\"rule\").toLowerCase();\n    const payload = {\n      id: this.account.id,\n      rule\n    }; // Dat cac thong tin thuc tap cua sinh vien vao Store de su dung.\n\n    await this.$store.dispatch(\"setFreelancerInfor\", payload);\n    await this.$store.dispatch(\"setProjectInfor\", payload);\n    await this.$store.dispatch(\"setProgressInfor\", payload);\n    await this.$store.dispatch(\"setProjectAssess\", payload);\n    await this.$store.dispatch(\"setAdviserAssess\", payload);\n  },\n\n  mounted() {\n    const viewProgress = document.querySelector(\".view-progress\");\n    const viewAssessProject = document.querySelector(\".view-assess-project\");\n    const viewAssessAdviser = document.querySelector(\".view-assess-adviser\");\n    this.clearDisabled();\n\n    if (viewProgress) {\n      const btnProgress = document.querySelector(\".btn-progress\");\n      btnProgress.disabled = true;\n    }\n\n    if (viewAssessProject) {\n      const btnProject = document.querySelector(\".btn-project\");\n      btnProject.disabled = true;\n    }\n\n    if (viewAssessAdviser) {\n      const btnAdviser = document.querySelector(\".btn-adviser\");\n      btnAdviser.disabled = true;\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAuFA,OAAOA,cAAP,MAA2B,iDAA3B;AACA,OAAOC,YAAP,MAAyB,mDAAzB;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,eAAe;EACbC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CADC;EAGbG,QAAQ,EAAE,EACR,GAAGF,UAAU,CAAC;MAAEG,OAAO,EAAE,YAAX;MAAyBC,UAAU,EAAE;IAArC,CAAD;EADL,CAHG;EAObC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,MAAMC,OAAM,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,2BAA1B,CAAhB;MACAF,OAAO,CAACG,OAAR,CAAiBC,MAAD,IAAY;QAC1BA,MAAM,CAACC,QAAP,GAAkB,KAAlB;MACD,CAFD;IAGD,CANM;;IAOPC,mBAAmB,CAACC,KAAD,EAAQ;MACzB,KAAKR,aAAL;MACAQ,KAAK,CAACC,aAAN,CAAoBH,QAApB,GAA+B,IAA/B;IACD;;EAVM,CAPI;;EAoBb,MAAMI,OAAN,GAAgB;IACd,MAAM,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,YAArB,CAAN;IACA,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,MAArB,EAA6BC,WAA7B,EAAb;IACA,MAAMC,OAAM,GAAI;MAAEC,EAAE,EAAE,KAAKrB,OAAL,CAAaqB,EAAnB;MAAuBL;IAAvB,CAAhB,CAHc,CAKd;;IACA,MAAM,KAAKF,MAAL,CAAYC,QAAZ,CAAqB,oBAArB,EAA2CK,OAA3C,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,iBAArB,EAAwCK,OAAxC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;IACA,MAAM,KAAKN,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCK,OAAzC,CAAN;EACD,CA/BY;;EAgCbE,OAAO,GAAG;IACR,MAAMC,YAAW,GAAIlB,QAAQ,CAACmB,aAAT,CAAuB,gBAAvB,CAArB;IACA,MAAMC,iBAAgB,GAAIpB,QAAQ,CAACmB,aAAT,CAAuB,sBAAvB,CAA1B;IACA,MAAME,iBAAgB,GAAIrB,QAAQ,CAACmB,aAAT,CAAuB,sBAAvB,CAA1B;IACA,KAAKrB,aAAL;;IACA,IAAIoB,YAAJ,EAAkB;MAChB,MAAMI,WAAU,GAAItB,QAAQ,CAACmB,aAAT,CAAuB,eAAvB,CAApB;MACAG,WAAW,CAAClB,QAAZ,GAAuB,IAAvB;IACF;;IACA,IAAIgB,iBAAJ,EAAuB;MACrB,MAAMG,UAAS,GAAIvB,QAAQ,CAACmB,aAAT,CAAuB,cAAvB,CAAnB;MACAI,UAAU,CAACnB,QAAX,GAAsB,IAAtB;IACF;;IACA,IAAIiB,iBAAJ,EAAuB;MACrB,MAAMG,UAAS,GAAIxB,QAAQ,CAACmB,aAAT,CAAuB,cAAvB,CAAnB;MACAK,UAAU,CAACpB,QAAX,GAAsB,IAAtB;IACF;EACD;;AAjDY,CAAf","names":["TitleStructure","InforProject","mapGetters","components","computed","account","freelancer","methods","clearDisabled","buttons","document","querySelectorAll","forEach","button","disabled","viewInforInternship","event","currentTarget","created","$store","dispatch","rule","localStorage","getItem","toLowerCase","payload","id","mounted","viewProgress","querySelector","viewAssessProject","viewAssessAdviser","btnProgress","btnProject","btnAdviser"],"sourceRoot":"","sources":["/Users/docaominh/Desktop/NLNKTPM/SUPLANCER/frontend/src/views/freelancer/FreelancerManage.vue"],"sourcesContent":["<template>\n  <div>\n    <div\n      class=\"p-4 animate__animated animate__fadeIn container\"\n      v-if=\"freelancer.projectId !== null\"\n    >\n      <div class=\"col-12 d-flex\">\n        <TitleStructure\n          :title=\"`Tên dự án làm việc`\"\n          class=\"col-6 me-4\"\n        ></TitleStructure>\n        <TitleStructure\n          :title=\"`Quản lý công việc`\"\n          class=\"col-6\"\n        ></TitleStructure>\n      </div>\n      <div>\n        <div\n          class=\"infor row m-0 pt-3 pb-5 border-bottom justify-content-between\"\n        >\n          <InforProject></InforProject>\n          <div\n            class=\"manage-internship col-6 d-flex justify-content-center align-items-center mb-4 mx-auto\"\n          >\n            <div class=\"col-3 me-5\">\n              <router-link\n                to=\"/freelancer/manage/view-progress\"\n                class=\"router-link\"\n              >\n                <button\n                  type=\"button\"\n                  class=\"btn btn-progress\"\n                  @click=\"viewInforInternship($event)\"\n                >\n                  <i class=\"bi bi-calendar-week fs-2 me-2\"></i>\n                  <span>Xem tiến độ công việc</span>\n                </button>\n              </router-link>\n            </div>\n            <div class=\"col-3 me-5\">\n              <router-link\n                to=\"/freelancer/manage/view-assess-project\"\n                class=\"router-link\"\n              >\n                <button\n                  type=\"button\"\n                  class=\"btn btn-project\"\n                  @click=\"viewInforInternship($event)\"\n                >\n                  <i class=\"bi bi-clipboard-check fs-2 me-2\"></i>\n                  <span>Xem đánh giá của đơn vị dự án</span>\n                </button>\n              </router-link>\n            </div>\n            <div class=\"col-3\">\n              <router-link\n                to=\"/freelancer/manage/view-assess-adviser\"\n                class=\"router-link\"\n              >\n                <button\n                  type=\"button\"\n                  class=\"btn btn-adviser\"\n                  @click=\"viewInforInternship($event)\"\n                >\n                  <i class=\"bi bi-person-video3 fs-2 me-2\"></i>\n                  <span>Xem đánh giá của Adviser</span>\n                </button>\n              </router-link>\n            </div>\n          </div>\n        </div>\n        <router-view></router-view>\n      </div>\n    </div>\n    <div class=\"mt-5 text-center no-info\" v-else>\n      <img\n        class=\"mt-5\"\n        src=\"https://cdn-icons-png.flaticon.com/512/4076/4076389.png\"\n        alt=\"\"\n        style=\"width: 300px\"\n      />\n      <h3 class=\"text-warning mt-4\">Freelancer chưa tham gia dự án</h3>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nimport InforProject from \"../../components/GlobalComponent/InforProject.vue\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  components: { TitleStructure, InforProject },\n\n  computed: {\n    ...mapGetters({ account: \"getAccount\", freelancer: \"getFreelancerInfor\" }),\n  },\n\n  methods: {\n    clearDisabled() {\n      const buttons = document.querySelectorAll(\".manage-internship button\");\n      buttons.forEach((button) => {\n        button.disabled = false;\n      });\n    },\n    viewInforInternship(event) {\n      this.clearDisabled();\n      event.currentTarget.disabled = true;\n    },\n  },\n\n  async created() {\n    await this.$store.dispatch(\"setAccount\");\n    const rule = localStorage.getItem(\"rule\").toLowerCase();\n    const payload = { id: this.account.id, rule };\n\n    // Dat cac thong tin thuc tap cua sinh vien vao Store de su dung.\n    await this.$store.dispatch(\"setFreelancerInfor\", payload);\n    await this.$store.dispatch(\"setProjectInfor\", payload);\n    await this.$store.dispatch(\"setProgressInfor\", payload);\n    await this.$store.dispatch(\"setProjectAssess\", payload);\n    await this.$store.dispatch(\"setAdviserAssess\", payload);\n  },\n  mounted() {\n    const viewProgress = document.querySelector(\".view-progress\");\n    const viewAssessProject = document.querySelector(\".view-assess-project\");\n    const viewAssessAdviser = document.querySelector(\".view-assess-adviser\");\n    this.clearDisabled();\n    if (viewProgress) {\n      const btnProgress = document.querySelector(\".btn-progress\");\n      btnProgress.disabled = true;\n    }\n    if (viewAssessProject) {\n      const btnProject = document.querySelector(\".btn-project\");\n      btnProject.disabled = true;\n    }\n    if (viewAssessAdviser) {\n      const btnAdviser = document.querySelector(\".btn-adviser\");\n      btnAdviser.disabled = true;\n    }\n  },\n};\n</script>\n\n<style scoped>\n.router-link {\n  cursor: default;\n  text-decoration: none;\n}\n\n.btn {\n  height: 90px;\n  width: 180px;\n  display: flex;\n  align-items: center;\n}\na {\n  color: #95a800;\n}\n.btn:first-child:active {\n  background-color: #95a800 !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}